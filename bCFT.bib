	keywords = {High Energy Physics - Theory},
	annote = {Comment: Expanded version of lectures given at the Summer School and Conference Nonperturbative Quantum Field Theoretic Methods and their Applications, August 2000, Budapest, Hungary. 35 pages},
	annote = {has very elegant proof of Ishibashi state due to G. Watt.},
	file = {arXiv\:hep-th/0103007 PDF:/home/tzhou13/.mozilla/firefox/fpdnafbj.default-1441422675402/zotero/storage/KZE23XQZ/Petkova and Zuber - 2001 - Conformal Boundary Conditions and what they teach .pdf:application/pdf;arXiv.org Snapshot:/home/tzhou13/.mozilla/firefox/fpdnafbj.default-1441422675402/zotero/storage/96JNAV2I/0103007.html:text/html}
