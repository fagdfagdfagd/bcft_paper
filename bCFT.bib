

@article{affleck_boundary_1997,
	title = {Boundary {Condition} {Changing} {Operators} in {Conformal} {Field} {Theory} and {Condensed} {Matter} {Physics}},
	volume = {58},
	issn = {09205632},
	url = {http://arxiv.org/abs/hep-th/9611064},
	doi = {10.1016/S0920-5632(97)00411-8},
	abstract = {Boundary condition changing operators in conformal field theory describe various types of "sudden switching" problems in condensed matter physics such as the X-ray edge singularity. We review this subject and give two extensions of previous work. A general derivation of a connection between the X-ray edge singularity, the Anderson orthogonality catastrophe and finite-size scaling of energies is given. The formalism is also extended to include boundstates.},
	urldate = {2017-03-24},
	journal = {Nuclear Physics B - Proceedings Supplements},
	author = {Affleck, Ian},
	month = sep,
	year = {1997},
	note = {arXiv: hep-th/9611064},
	keywords = {Condensed Matter, High Energy Physics - Theory},
	pages = {35--41},
	annote = {Comment: 7 pages, RevTex, epsf, for the proceedings of the conference, "Advanced Quantum Field Theory" (in memory of Claude Itzykson) La Londe, France, September, 1996},
	annote = {This paper has three parts:
1. relating the bcc scaling with Casimir energy(ground state energy difference) through a conformal mapping.
2. X-ray edge singularity: when X-ray hit the metal and kick out a core electron, the resulting hole will change the potential for the conduction electron. In the IR, the effective is captured by a phase shift in the Dirac electron and hence a bcc effect.
It seems that the phase shift conformal interface always gives quadratic scaling dimensions.
3. Kondo problem with spin. The scaling dimension is given by the Kac-Moody algebra. I don't quite understand this calculation. },
	file = {arXiv\:hep-th/9611064 PDF:/home/tzhou13/.mozilla/firefox/fpdnafbj.default-1441422675402/zotero/storage/MMADBCIT/Affleck - 1997 - Boundary Condition Changing Operators in Conformal.pdf:application/pdf;arXiv.org Snapshot:/home/tzhou13/.mozilla/firefox/fpdnafbj.default-1441422675402/zotero/storage/C8CBEKU9/9611064.html:text/html}
}



@article{eggert_magnetic_1992,
	title = {Magnetic impurities in half-integer-spin {Heisenberg} antiferromagnetic chains},
	volume = {46},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.46.10866},
	doi = {10.1103/PhysRevB.46.10866},
	abstract = {We study the effect of an isolated impurity on the low-energy properties of a half-odd-integer-spin Heisenberg antiferromagnetic chain using both numerical and conformal field theory techniques. The impurity corresponds to the substitution of a magnetic ion by a different ion with the same or different spin, or else to the coupling of a magnetic impurity to a single spin on the chain. Depending on which kind of impurity is present, the low-energy behavior corresponds either to a ‘‘healing’’ of the defect or else to a severing of the chain at the impurity location. In both cases there is a coupling-dependent length scale over which magnetic screening takes place. Analogies with the Kondo effect are elucidated.},
	number = {17},
	urldate = {2017-04-02},
	journal = {Physical Review B},
	author = {Eggert, Sebastian and Affleck, Ian},
	month = nov,
	year = {1992},
	pages = {10866--10883},
	annote = {very clear derivation from XXZ -{\textgreater} Luttinger Liquid and impurity analysis.},
	file = {APS Snapshot:/home/tzhou13/.mozilla/firefox/fpdnafbj.default-1441422675402/zotero/storage/ETQGMDM4/PhysRevB.46.html:text/html;PhysRevB.46.10866.pdf:/home/tzhou13/.mozilla/firefox/fpdnafbj.default-1441422675402/zotero/storage/BRUQH23M/PhysRevB.46.10866.pdf:application/pdf}
